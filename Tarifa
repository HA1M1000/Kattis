import java.util.Scanner;
public class Tarifa
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int x = sc.nextInt();
        int n = sc.nextInt();
        int temp = 0;

        int array[] = new int[n];
        for(int i = 0; i< array.length; i++)
        {
            array[i] = sc.nextInt();
        }
        for(int i = 0; i< array.length; i++)
        {
            temp = temp + (x - array[i]);
        }
        System.out.println(temp + x);

    }
}
